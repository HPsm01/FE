{
  "compilerOptions": {
    "jsx": "react-native",
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node16",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "node16",
    "target": "es2016",
    "types": ["react-native"],
    "lib": ["dom", "esnext"],
    "resolveJsonModule": true,
    "noEmit": true
  },
  "extends": "@tsconfig/react-native/tsconfig.json"
}
